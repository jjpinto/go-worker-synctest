{
  "name": "Go Worker Demo",
  "image": "mcr.microsoft.com/devcontainers/go:1.23",
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go"
      ],
      "settings": {
        "go.toolsManagement.autoUpdate": true,
        "go.useLanguageServer": true,
        "editor.formatOnSave": true,
        "go.formatTool": "gofmt"
      }
    }
  },
  "postCreateCommand": "go version"
}